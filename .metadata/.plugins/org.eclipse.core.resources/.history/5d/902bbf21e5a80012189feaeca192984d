package com.ivinny.json;

public enum JsonData {
	Boston("Boston"),
	Jacksonville("Jacksonville"),
	Atlanta("Atlanta"),
	SanDiego("SanDiego"),
	Dallas("Dallas");
	
	private String type;

	JsonData (String i){
		this.type = i;
	}
	
	public String getStringType(){
		return type;
	}
	public boolean isCold(int tempFarhenheit){
		
		if (tempFarhenheit < 30){
			return true;
		}else{
			return false;
		}
	}
}
