package com.ivinny.json;

public class Json {
	//simple json string (creating data)
	public static String getJsonString(){
		return "{Cities: " +
				"[{name: Boston, temp: 21}," +
				"{name: Jacksonville, temp: 60}," +
				"{name: Atlanta, temp: 29}," +
				"{name: SanDiego, temp: 75}," +
				"{name: Dallas, temp: 41}]" +
				"}";
	}
}
